package com.company;

public class Main {


    public static int Zadanie5(int liczba1, int liczba2)

    {

        while (liczba1 != liczba2)

        {


            if (liczba1 > liczba2)


            {


                liczba1 -= liczba2;


            }

            else

            {

                liczba2 -= liczba1;


            }

        }
        return liczba1;
    }

    public static void main(String[] args)


    {


        System.out.println("Wynik :" + Zadanie5(10, 15));

        //Norbert Kulik
    }

}
